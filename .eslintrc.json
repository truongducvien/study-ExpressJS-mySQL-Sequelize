{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "linebreak-style": "off",
    "comma-dangle": "off",
    "import/no-extraneous-dependencies": "off",
    "no-plusplus": "off"
  }
}
